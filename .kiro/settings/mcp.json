{
  "mcpServers": {
    "knowledge-mcp.global.api.aws": {
      "command": "npx",
      "args": [
        "-y",
        "@aws/amazon-q-developer-cli-mcp-server"
      ]
    },
    "awslabs.frontend-mcp-server": {
      "command": "npx",
      "args": [
        "-y",
        "@aws/mcp-server-aws-frontend"
      ]
    },
    "aws-cdk": {
      "command": "npx",
      "args": [
        "-y",
        "@aws/mcp-server-aws-cdk"
      ]
    },
    "fetch": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-fetch"
      ]
    }
  }
}
